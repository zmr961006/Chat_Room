#include<stdio.h>
#include<gtk/gtk.h>
#include"../srv/serve.h"
#include"./service.h"
#include"./main_show.h"
int main(int argc,char **argv)
{
   
    GtkWidget *window;
    
    if(!g_thread_supported())
 	g_thread_init(NULL);
    gtk_init(&argc,&argv);
    window = show_ser();
    gtk_widget_show_all(window);
    gtk_main();
    return 0;
}



